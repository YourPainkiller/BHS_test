        },
        "version": "1.0"
    },
    "host": "localhost:4000",
    "paths": {
        "/api/auth/add": {
            "post": {
                "description": "Adding asset to store",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "asset"
                ],
                "summary": "Add asset",
                "parameters": [
                    {
                        "description": "asset info",
                        "name": "AssetData",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.Add"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/domain.AcceptResponse"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/domain.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/api/auth/buy": {
            "post": {
                "description": "Buying asset from store",
