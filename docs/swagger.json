    "definitions": {
        "domain.AcceptResponse": {
            "type": "object",
            "properties": {
                "detail": {
                    "type": "string",
                    "example": "success"
                }
            }
        },
        "domain.ErrorResponse": {
            "type": "object",
            "properties": {
                "message": {
                    "type": "string",
                    "example": "error message"
                }
            }
        },
        "dto.Add": {
            "type": "object",
            "properties": {
                "assetDescr": {
                    "type": "string",
                    "example": "lorem ipsum"
                },
                "assetName": {
                    "type": "string",
                    "example": "tree"
                },
                "assetPrice": {
                    "type": "integer",
                    "example": 10
                }
            }
        },
        "dto.Buy": {
            "type": "object",
            "properties": {
                "assetName": {
                    "type": "string",
                    "example": "tree"
                },
                "count": {
                    "type": "integer",
                    "example": 3
                }
            }
        },
        "dto.Delete": {
            "type": "object",
            "properties": {
                "assetName": {
                    "type": "string",
                    "example": "tree"
                }
            }
        },
        "dto.UserCredentials": {
            "type": "object",
            "properties": {
                "password": {
                    "type": "string",
                    "example": "yourpassword"
                },
                "username": {
                    "type": "string",
                    "example": "yourusername"
                }
            }
        }
    }
}