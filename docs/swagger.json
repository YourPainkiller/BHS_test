                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "asset"
                ],
                "summary": "Buy asset",
                "parameters": [
                    {
                        "description": "asset name and count",
                        "name": "AssetData",
                        "in": "body",
                        "required": true,
