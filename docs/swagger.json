        "/api/auth/register": {
            "post": {
                "description": "With this command you can register user",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "auth"
                ],
                "summary": "Register user",
                "parameters": [
                    {
                        "description": "your credentils",
                        "name": "UserData",
                        "in": "body",
                        "required": true,
